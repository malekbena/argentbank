{
    "version": 2,
    "builds": [
        {
            "src": "./server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "headers": {
                "Access-Control-Allow-Origin": "*"
              },
              "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "OPTIONS",
                "DELETE",
                "HEAD",
                "CONNECT",
                "TRACE"
            ]
        }
    ]
}